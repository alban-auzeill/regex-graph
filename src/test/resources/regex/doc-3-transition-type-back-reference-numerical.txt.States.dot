  6 -> 3 [ label="",  taillabel="group" ]

  // backreference-successor edges
  edge [fontname="Monospace", fontsize="9", style="bold", color="DodgerBlue", fontcolor="MediumBlue", arrowhead="boxvee", arrowtail="none", dir="both"]
