digraph G {
  rankdir=LR;
  graph [fontname="Monospace", fontsize="13"]

  // default nodes
  node [fontname="Monospace", fontsize="9", shape="box", style="rounded,filled", color="LightGray", fillcolor="Beige"]
  1[ label="GraphWriterTest$B:1\{\l  .b: 0\l\}\l" ]
  2[ label="GraphWriterTest$C:2\{\l\}\l" ]
  3[ label="GraphWriterTest$C:3\{\l\}\l" ]
  4[ label="GraphWriterTest$C:4\{\l\}\l" ]

  // default edges
  edge [fontname="Monospace", fontsize="9", color="Navy"]
  3 -> 2 [ label="",  taillabel="previous" ]
  4 -> 2 [ label="",  taillabel="previous" ]
  4 -> 4 [ label="",  taillabel="next" ]
  3 -> 4 [ label="",  taillabel="next" ]
  2 -> 3 [ label="",  taillabel="previous" ]
  2 -> 4 [ label="",  taillabel="next" ]
  1 -> 2 [ label="",  taillabel="a" ]
  1 -> 2 [ label="",  taillabel="c" ]
}
